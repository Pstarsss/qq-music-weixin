.homeContainer{
  background-color: #f1f1f1;
  min-height: 100vh;
  box-sizing: border-box;
  padding:0 32rpx;
}
.header{
  padding:10rpx 0;
}
.searchTip{
  background-color: #FFFFFF;
  margin: 0 30rpx;
  border-radius: 16rpx;
  padding: 10rpx 2rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.searchTip text{
  font-size:20rpx;
}
.Toplist-text{
  font-weight: 500;
  margin:20rpx 0;
}
.R_head{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0 20rpx 0;
}
.R_head_title{
  font-weight: 500;
  font-size: 32rpx;
}
.R_head_more_text{
  font-size: 24rpx;
  color:#a9a9a9;
}
.R_head_more{
  display: flex;
  align-items: center;
}
.R_list_item{
  display: flex;
  align-items: center;
  padding-bottom: 10rpx;
}
.R_item_info{
  flex:1;
  padding: 0 20rpx;
}
.R_item_img image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 18rpx;
}