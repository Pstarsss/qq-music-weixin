.categoryGroup{
  padding:20rpx 0;
  width:100%;
}
.categoryGroupName{
  font-size: 36rpx;
  font-weight: 500;
}
.categoryGroupList{
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  justify-content: space-between;
}
.categoryGroupitem{
  background-color: #fff;
  border-radius: 30rpx;
  width: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  margin-top: 20rpx;
}