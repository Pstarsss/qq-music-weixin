.nav-bar-custom{
  width: 40%;
  box-sizing: border-box;
  height: 64rpx;
  background: rgba(255, 255, 255, 0.6);
  border-radius: 50rpx;
  border: 1rpx solid rgba(151, 151, 151, 0.2);
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.nav-bar-custom .line{
  width: 1rpx;
  color: rgba(0, 0, 0, 0.2);
  height: 37rpx;
  line-height: 30rpx;
}

.nav-bar-custom .back-icon,
.nav-bar-custom .home-icon {
  width: 48rpx;
  height: 48rpx;
}